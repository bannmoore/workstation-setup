{
	"react_component": {
		"prefix": "bam_react_component",
		"body": [
			"const React = require('react')",
			"const PropTypes = require('prop-types')",
			"",
			"class $1 extends React.Component {",
			"  constructor(props) {",
			"    super(props)",
			"  }",
			"",
			"  render() {",
			"    return (",
			"      <div>$1</div>",
			"    )",
			"  }",
			"}",
			"",
			"$1.propTypes={}",
			"",
			"module.exports = $1"
		],
		"description": "React component"
	},
	"react_es6_component": {
		"prefix": "bam_react_es6_component",
		"body": [
			"import React from 'react'",
			"import PropTypes from 'prop-types'",
			"",
			"class $1 extends React.Component {",
			"  constructor(props) {",
			"    super(props)",
			"  }",
			"",
			"  render() {",
			"    return (",
			"      <div>$1</div>",
			"    )",
			"  }",
			"}",
			"",
			"$1.propTypes = {}",
			"",
			"export default $1"
		],
		"description": "React component (ES6)"
	},
	"react_functional_component": {
		"prefix": "bam_react_functional_component",
		"body": [
			"import React from 'react'",
			"",
			"export const $1 = () => <div>$1</div>"
		]
	},
	"react_test": {
		"prefix": "bam_react_test",
		"body": [
			"const React = require('react')",
			"const { shallow } = require('enzyme')",
			"const $1 = require('./$1')",
      "",
			"describe('$1', () => {",
  		"  it('shallow renders without crashing', () => {",
    	"    shallow(<$1 />)",
  		"  })",
			"})"
		],
		"description": "React test"
	},
